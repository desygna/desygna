{
  "private": true,
  "scripts": {
    "changeset": "changeset",
    "w": "yarn workspaces foreach --parallel --topological --verbose",
    "w:build": "yarn w --no-private run p:build",
    "w:brl": "yarn w --no-private run p:brl",
    "docs:build": "cd docs && yarn && yarn build",
    "docs:start": "cd docs && yarn && yarn start",
    "p:build": "cd $INIT_CWD && tsc && yarn p:rollup",
    "p:build:watch": "cd $INIT_CWD && yarn p:rollup --watch",
    "p:brl": "cd $INIT_CWD && barrelsby -d src -D -l all -q -e '.*(template|spec|__tests__|stories|private).*'",
    "p:rollup": "cd $INIT_CWD && rollup -c"
  },
  "workspaces": [
    "packages/**"
  ],
  "resolutions": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "@types/react": "17.0.47",
    "@types/react-dom": "17.0.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/runtime": "^7.15.3",
    "@changesets/changelog-github": "^0.4.6",
    "@changesets/cli": "^2.24.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-url": "^7.0.0",
    "@types/react": "17.0.47",
    "@types/react-dom": "17.0.17",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "barrelsby": "^2.4.0",
    "lerna": "^3.22.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "tslint": "^5.20.1",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.0"
  },
  "packageManager": "yarn@3.2.3"
}
